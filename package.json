{
    "name": "@dffp/monorepo",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "ISC",
    "type": "module",
    "scripts": {

        "types": "npm run types -ws",
        "build": "npm run build -ws",

        "lint": "eslint .",
        "fixlint": "eslint . --fix",

        "sso": "aws sso login --profile Admin",
        "exsso": "aws configure export-credentials --profile Admin --format env",

        "format": "terraform -chdir=terraform fmt -recursive",
        "plan": "terraform -chdir=terraform plan -var-file=.qual -out .qual.tfplan",
        "apply": "terraform -chdir=terraform apply .qual.tfplan",

        "postinstall": "husky"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.632.0",
        "@aws-sdk/lib-dynamodb": "3.632.0",
        "@hapi/boom": "10.0.1",
        "clashofclans.js": "3.3.12",
        "discord-interactions": "4.0.0",
        "joi": "17.13.3"
    },
    "devDependencies": {
        "@aws-sdk/client-ssm": "3.632.0",
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.2.2",
        "@eslint/js": "9.9.0",
        "@stylistic/eslint-plugin": "2.6.4",
        "@stylistic/eslint-plugin-plus": "2.6.4",
        "@stylistic/eslint-plugin-js": "2.6.4",
        "@stylistic/eslint-plugin-ts": "2.6.4",
        "@types/aws-lambda": "8.10.140",
        "@types/eslint__js": "8.42.3",
        "@types/node": "22.4.1",
        "esbuild": "0.20.2",
        "eslint": "8.57.0",
        "husky": "9.0.11",
        "tsx": "4.16.2",
        "typescript": "5.4.5",
        "typescript-eslint": "8.2.0",
        "vite": "5.2.10",
        "vitest": "1.5.2"
    },
    "workspaces": [
        "cli"
    ]
}
